[
  {
    "name": "meadow",
    "image": "nulib/meadow:${docker_tag}",
    "cpu": ${cpu_reservation},
    "memoryReservation": ${memory_reservation},
    "mountPoints": [],
    "essential": true,
    "environment": [
      {
        "name": "MEADOW_PROCESSES",
        "value": "${processes}"
      },
      {
        "name": "SECRET_KEY_BASE",
        "value": "${secret_key_base}"
      },
      {
        "name": "DIGITAL_COLLECTIONS_URL",
        "value": "${digital_collections_url}"
      },
      {
        "name": "DATABASE_URL",
        "value": "${database_url}"
      },
      {
        "name": "HONEYBADGER_API_KEY",
        "value": "${honeybadger_api_key}"
      },
      {
        "name": "ELASTICSEARCH_KEY",
        "value": "${elasticsearch_key}"
      },
      {
        "name": "ELASTICSEARCH_SECRET",
        "value": "${elasticsearch_secret}"
      },
      {
        "name": "ELASTICSEARCH_URL",
        "value": "${elasticsearch_url}"
      },
      {
        "name": "EZID_PASSWORD",
        "value": "${ezid_password}"
      },
      {
        "name": "EZID_SHOULDER",
        "value": "${ezid_shoulder}"
      },
      {
        "name": "EZID_TARGET_BASE_URL",
        "value": "${ezid_target_base_url}"
      },
      {
        "name": "EZID_USER",
        "value": "${ezid_user}"
      },
      {
        "name": "GEONAMES_USERNAME",
        "value": "${geonames_username}"
      },
      {
        "name": "IIIF_MANIFEST_URL",
        "value": "${iiif_manifest_url}"
      },
      {
        "name": "IIIF_SERVER_URL",
        "value": "${iiif_server_url}"
      },
      {
        "name": "INGEST_BUCKET",
        "value": "${ingest_bucket}"
      },
      {
        "name": "LDAP_BASE_DN",
        "value": "${ldap_base_dn}"
      },
      {
        "name": "LDAP_BIND_DN",
        "value": "${ldap_bind_dn}"
      },
      {
        "name": "LDAP_BIND_PASSWORD",
        "value": "${ldap_bind_password}"
      },
      {
        "name": "LDAP_PORT",
        "value": "${ldap_port}"
      },
      {
        "name": "LDAP_SERVER",
        "value": "${ldap_server}"
      },
      {
        "name": "MEADOW_HOSTNAME",
        "value": "${host_name}"
      },
      {
        "name": "DB_POOL_SIZE",
        "value": "${db_pool_size}"
      },
      {
        "name": "DB_QUEUE_TARGET",
        "value": "${db_queue_target}"
      },
      {
        "name": "DB_QUEUE_INTERVAL",
        "value": "${db_queue_interval}"
      },
      {
        "name": "MEDIACONVERT_QUEUE",
        "value": "${mediaconvert_queue}"
      },
      {
        "name": "MEDIACONVERT_ROLE",
        "value": "${mediaconvert_role}"
      },
      {
        "name": "PRESERVATION_BUCKET",
        "value": "${preservation_bucket}"
      },
      {
        "name": "PRESERVATION_CHECK_BUCKET",
        "value": "${preservation_check_bucket}"
      },
      {
        "name": "PYRAMID_BUCKET",
        "value": "${pyramid_bucket}"
      },
      {
        "name": "PYRAMID_TIFF_WORKING_DIR",
        "value": "/working"
      },
      {
        "name": "RELEASE_COOKIE",
        "value": "${secret_key_base}"
      },
      {
        "name": "RELEASE_DISTRIBUTION",
        "value": "name"
      },
      {
        "name": "STREAMING_BUCKET",
        "value": "${streaming_bucket}"
      },
      {
        "name": "STREAMING_URL",
        "value": "${streaming_url}"
      },
      {
        "name": "UPLOAD_BUCKET",
        "value": "${upload_bucket}"
      },
      {
        "name": "WORK_ARCHIVER_ENDPOINT",
        "value": "${work_archiver_endpoint}"
      },
      {
        "name": "AGENTLESS_SSO_KEY",
        "value": "${agentless_sso_key}"
      },
      {
        "name": "SITEMAP_BUCKET",
        "value": "${digital_collections_bucket}"
      },
      {
        "name": "ALLOWED_ORIGINS",
        "value": "${meadow_urls}"
      },
      {
        "name": "INGEST_FILE_SET_PROCESSOR_CONCURRENCY",
        "value": "20"
      },
      {
        "name": "EXTRACT_MIME_TYPE_PROCESSOR_CONCURRENCY",
        "value": "20"
      },
      {
        "name": "INITIALIZE_DISPATCH_PROCESSOR_CONCURRENCY",
        "value": "20"
      },
      {
        "name": "DISPATCHER_PROCESSOR_CONCURRENCY",
        "value": "20"
      },
      {
        "name": "GENERATE_FILE_SET_DIGESTS_PROCESSOR_CONCURRENCY",
        "value": "20"
      },
      {
        "name": "EXTRACT_EXIF_METADATA_PROCESSOR_CONCURRENCY",
        "value": "20"
      },
      {
        "name": "COPY_FILE_TO_PRESERVATION_PROCESSOR_CONCURRENCY",
        "value": "20"
      },
      {
        "name": "CREATE_PYRAMID_TIFF_PROCESSOR_CONCURRENCY",
        "value": "20"
      },
      {
        "name": "EXTRACT_MEDIA_METADATA_PROCESSOR_CONCURRENCY",
        "value": "20"
      },
      {
        "name": "CREATE_TRANSCODE_JOB_PROCESSOR_CONCURRENCY",
        "value": "20"
      },
      {
        "name": "TRANSCODE_COMPLETE_PROCESSOR_CONCURRENCY",
        "value": "20"
      },
      {
        "name": "GENERATE_POSTER_IMAGE_PROCESSOR_CONCURRENCY",
        "value": "20"
      },
      {
        "name": "FILE_SET_COMPLETE_PROCESSOR_CONCURRENCY",
        "value": "20"
      }    ],
    "portMappings": [
      {
        "containerPort": 4000,
        "hostPort": 4000,
        "protocol": "tcp"
      },
      {
        "containerPort": 4369,
        "hostPort": 4369,
        "protocol": "tcp"
      },
      {
        "containerPort": 24601,
        "hostPort": 24601,
        "protocol": "tcp"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group}",
        "awslogs-region": "${region}",
        "awslogs-stream-prefix": "app"
      }
    },
    "ulimits": [
      {
        "name": "nofile",
        "softLimit": 65536,
        "hardLimit": 65536
      }
    ],
    "volumesFrom": []
  }
]
